<table class="table-unstyle">
  <caption>{{localize "Witcher.Actor.DebugEffects.Info"}}</caption>
  <tr>
    <th colspan="2">{{localize "Witcher.Item.Effects.Names"}}</th>
    <th>{{localize "Witcher.Item.Effects.Category"}}</th>
    <th>{{localize "Witcher.Item.Effects.Activity"}}</th>
    <th>{{{localize "Witcher.Item.Effects.onToken"}}}</th>
    <th>{{{localize "Witcher.Item.Effects.Target"}}}</th>
    <th colspan="2">{{localize "Witcher.Item.Effects.Things"}}</th>
  </tr>
  {{#each data.data.modifications}}
  <tr data-id="{{id}}" data-effectId="{{effectId}}"{{#if status}} style="background-color:rgb(186, 240, 198, 0.3)"{{/if}}>
    <td><img src="{{img}}" title="{{name}}"></td>
    <td>{{name}}</td>
    <td>{{lzCc "Witcher.Item.Effects.Categories." category}}</td>
    <td>{{lzCc "Witcher.Item.Effects.Activities." activity}}</td>
    <td>{{lzCc "Witcher.Item.Effects.onTokens." onToken}}</td>
    <td>{{lzCc "Witcher.Item.Effects.Targets." target}}</td>
    <td><img src="{{info.img}}" title="{{info.name}}"></td>
    <td>{{info.name}}</td>
  </tr>
  <tr{{#if status}} style="background-color:rgb(186, 240, 198, 0.3)"{{/if}}>
    <td colspan="4">{{localize "Witcher.Item.Effects.Formulas"}}: {{stringify formula}}</td>
    <td colspan="4">{{localize "Witcher.Item.Effects.Saving"}}: {{stringify saving}}</td>
  </tr>
  {{/each}}
</table>