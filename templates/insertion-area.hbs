<div class="witcher-insertion-area"
  data-item-id="{{data.id}}" 
  data-dist-field="{{dist_field}}" 
  data-dist-type="{{dist_type}}">
  {{#ifne data ""}}
    {{#isArray data}}
    <table>
      {{#each data}}
        <tr data-item-id="{{id}}">
          <td>
            <img class="img35x35 click-show" src="{{img}}" title="{{name}}">
          </td>
          <td>{{name}}</td>
          <td><a class="delete"><i class="fas fa-trash-alt"></i></a></td>
        </tr>
      {{/each}}
    </table>
    {{else}}
      <img class="img35x35 click-show" src="{{data.img}}" title="{{data.name}}">&nbsp;
      {{data.name}}&nbsp;
      <a class="delete"><i class="fas fa-trash-alt"></i></a>
    {{/isArray}}
  {{else}}
  {{localize "Witcher.DragDrop"}}
  {{/ifne}}
</div>